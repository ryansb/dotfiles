    Bundle 'jistr/vim-nerdtree-tabs'
    Bundle 'flazz/vim-colorschemes'
    Bundle 'vim-scripts/Gundo'
    Bundle 'myusuf3/numbers.vim'
    Bundle 'benmills/vimux'
